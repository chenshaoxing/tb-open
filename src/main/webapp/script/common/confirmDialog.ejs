<div class="modal fade">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h6 class="modal-title"><%=data.title%></h6>
      </div>
      <div class="modal-body">
        <div class="bordered"><%=data.message%></div>
      </div>
      <div class="modal-footer">
        <button type="button" id="subBtn<%=data.randomDialogId%>" class="btn btn-primary" data-dismiss="modal">确定</button>
        <button type="button" id="closeBtn<%=data.randomDialogId%>"  class="btn btn-default" data-dismiss="modal">关闭</button>
      </div>
    </div>
  </div>
</div>