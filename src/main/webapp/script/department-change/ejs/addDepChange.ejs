


<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content ">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                </button>
                <h4 class="modal-title" id="myModalLabel"><%=data.title%></h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" id="depChangeForm" method="post">
                    <input type="hidden" id="id" name="id" value="<%=data.id?data.id:''%>">
                    <% if(data.isAdd){%>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">员工:</label>
                                    <div class="col-sm-8">
                                        <select class="form-control required" id="empId" name="empId" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用日期</label>
                                    <div class="col-sm-8">
                                        <input  type="text" id="changeDate" name="changeDate" class="form-control  Wdate required " onFocus="WdatePicker({dateFmt:'yyyy-MM-dd'})"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">调动前部门:</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" readonly id="beforeDepName" name="beforeDepName" >
                                        <input type="hidden" class="form-control"   id="beforeDepId" name="beforeDepId" >
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用前职位</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" readonly id="beforePosition" name="beforePosition">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">调动后部门:</label>
                                    <div class="col-sm-8">
                                        <select class="form-control required" id="afterDepId" name="afterDepId" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用后职位</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control required"  id="afterPosition" name="afterPosition" >
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-9">
                                <div class="form-group">
                                    <label for="cause" class="col-sm-2 control-label">调动原因</label>
                                    <div class="col-sm-10">
                                        <input type="text" maxlength="200" class="form-control required "     id="cause" name="cause" >
                                    </div>
                                </div>
                            </div>
                        </div>

                    <%}else{%>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">员工:</label>
                                    <div class="col-sm-8">
                                        <select  class="form-control" disabled id="empIdS"  name="empId" >
                                        </select>
                                        <input type="hidden" name="empId" id="empId">
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用日期</label>
                                    <div class="col-sm-8">
                                        <input  type="text" id="changeDate" name="changeDate" class="form-control  Wdate required " value="<%=data.changeDate?common.fn.formatDate(data.changeDate):''%>" onFocus="WdatePicker({dateFmt:'yyyy-MM-dd'})"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">调动前部门:</label>
                                    <div class="col-sm-8">
                                        <select class="form-control" id="beforeDepId required" name="beforeDepId" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用前职位</label>
                                    <div class="col-sm-8">
                                        <input type="text" readonly id="beforePosition" class="form-control required" name="beforePosition" value="<%=data.beforePosition%>">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label  class="col-sm-4 control-label">调动后部门:</label>
                                    <div class="col-sm-8">
                                        <select class="form-control required" id="afterDepId" name="afterDepId" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-sm-4 control-label">调用后职位</label>
                                    <div class="col-sm-8">
                                        <input type="text"  id="afterPosition" class="form-control required" name="afterPosition" value="<%=data.afterPosition%>">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-9">
                                <div class="form-group">
                                    <label for="cause" class="col-sm-2 control-label">调动原因</label>
                                    <div class="col-sm-10">
                                        <input type="text" maxlength="200" class="form-control required "     id="cause" name="cause" value="<%=data.cause?data.cause:''%>" >
                                    </div>
                                </div>
                            </div>
                        </div>
                    <%}%>


                </form>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary"  onclick="depChange.fn.save('<%=data.randomDialogId%>');">保存</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            </div>
        </div>
    </div>
</div>
<style>
    .form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control {
        background-color: #f4f6f6;
        border-color: #d5dbdb;
        color: #34495e;
        cursor: default;
        opacity: 1;
        filter: alpha(opacity=70);
    }
</style>

