<% for(var i=0 ; i<data.list.length;i++){ %>
<tr>
    <td><%=data.list[i].buyerNickname%></td>

    <td>
    <span class="label label-lg  label-success  arrowed-right"><%=common.fn.formatDateTime(data.list[i].createdTime)%></span>
    </td>

    <td>
        <button class="btn btn-sm btn-danger" onclick="Black.fn.delete(<%=data.list[i].id%>)">
            <i class="fa fa-trash-o fa-1x"></i>
            删除
        </button>
    </td>
</tr>
<%}%>