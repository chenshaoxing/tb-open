<% for(var i=0 ; i<data.list.length;i++){ %>
<tr>
    <td><%=data.list[i].employee.id%></td>
    <td><%=data.list[i].employee.name%></td>
    <td><%=data.list[i].employee.department.name%></td>
    <td>
        <% if(data.list[i].contractType=='LABOR') {%>
        劳动合同
        <%}else{%>
        应聘合同
        <%}%>
    </td>
    <td>
        <% if(data.list[i].contractStatus == 'TIMEOUT') {%>
            过期
        <%}else{%>
            正常
        <%}%>
    </td>
    <td>
    <%=common.fn.formatDate(data.list[i].startDate)%>
    </td>
    <td>
    <%=common.fn.formatDate(data.list[i].endDate)%>
    </td>
    <td>
    <%=common.fn.formatDate(data.list[i].signDate)%>
    </td>
    <td>
    <input type="button" class="btn btn-primary" value="修改" onclick="contract.fn.getById(<%=data.list[i].id%>)">
    <input type="button" class="btn btn-primary" value="删除" onclick="applicant.fn.delete(<%=data.list[i].id%>)">
    </td>
</tr>
<%}%>