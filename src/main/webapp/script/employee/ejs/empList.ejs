<%for(var i=0 ; i<data.list.length ; i++){%>
<tr>
    <td><%=data.list[i].id%></td>
    <td><%=data.list[i].name%></td>
    <td><%=data.list[i].people%></td>
    <td><%=data.list[i].idCard%></td>
    <td><%=data.list[i].position%></td>
    <td><%=data.list[i].sex == "MAN"?"男":"女"%></td>
    <td><%=data.list[i].birthday%></td>
    <!--<td><%=data.list[i].eduName%></td>-->
    <!--<td><%=data.list[i].specialty%></td>-->
    <td><%=data.list[i].department.name%></td>
    <td><%=data.list[i].telephone%></td>
    <td><%=data.list[i].email%></td>
    <td>
        <input type="button" class="btn btn-primary" value="详情" onclick="employee.fn.showUpdateDialog(<%=data.list[i].id%>,true)">
        <input type="button" class="btn btn-primary" value="修改" onclick="employee.fn.showUpdateDialog(<%=data.list[i].id%>,false)">
        <input type="button" class="btn btn-primary" value="删除" onclick="employee.fn.deleteEmpById(<%=data.list[i].id%>)">
    </td>
</tr>
<%}%>