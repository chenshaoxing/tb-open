<%for(var i=0 ; i<data.list.length ; i++){%>
<tr>
    <td><%=data.list[i].applicantName%></td>
    <td><%=data.list[i].recruitNeed.id%></td>
    <td>
        <%if(data.list[i].sex == "MAN"){%>
           男
        <%}else{%>
        女
        <%}%>
    </td>
    <td><%=data.list[i].age%></td>
    <td><%=data.list[i].diploma%></td>
    <td><%=data.list[i].tel%></td>
    <td>
        <% if(data.list[i].applyStatus == 'CHU_SHI'){%>
        初试
        <%}else if(data.list[i].applyStatus == 'FU_SHI'){%>
        复试
        <%}else{%>
        结束
        <%}%>
    </td>
    <td>
        <%if(data.list[i].applyResult == 'PASS'){%>
            通过
        <%}else if(data.list[i].applyResult == 'NOT_PASS'){%>
            未通过
        <%}else{%>
           进行中
        <%}%>
    </td>
    <td>
        <input type="button" class="btn btn-primary" value="修改" onclick="applicant.fn.getById(<%=data.list[i].id%>)">
        <input type="button" class="btn btn-primary" value="删除" onclick="applicant.fn.delete(<%=data.list[i].id%>)">
    </td>
</tr>
<%}%>