<%for(var i=0 ; i<data.list.length ; i++){%>
<tr>
    <td><%=data.list[i].id%></td>
    <td><%=data.list[i].name%></td>
    <td><%=data.list[i].dep%></td>
    <td><%=common.fn.formatDateTime(data.list[i].createdTime)%></td>
    <td>
        <input type="button" class="btn btn-primary" value="修改" onclick="department.fn.getById(<%=data.list[i].id%>)">
        <input type="button" class="btn btn-primary" value="删除" onclick="department.fn.delete(<%=data.list[i].id%>)">
    </td>
</tr>
<%}%>